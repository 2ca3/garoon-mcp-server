# Garoon Configuration
# Copy this file to .env and fill in your actual credentials

# Your Garoon instance URL (without trailing slash)
GAROON_BASE_URL=https://your-subdomain.cybozu.com

# Your Garoon username (usually your email address)
GAROON_USERNAME=your.email@company.com

# Your Garoon password
GAROON_PASSWORD=your_password_here

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
